

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>

<div class="container">
  <div class="panel-center">
    <div class="card card-outline-secondary my-4">
      <div class="card-header">
        <h3 style="color:darkgreen"><Strong>Iniciar Sesion</strong></h3>
      </div>
      <div class="card-body">
        <div class="field">
             <strong><%= f.label :email %><br /></strong>
            <%= f.email_field :email, autofocus: true %>
        </div>
        <hr>
        <div class="field">
          <strong><%= f.label :password %><br /></strong>
          <%= f.password_field :password, autocomplete: "off" %>
        </div>
        <hr>
          <div class="actions">
        <%= f.submit "Iniciar sesion", class: "btn btn-primary" %>
        </div>
          <% if devise_mapping.rememberable? -%>
        <div class="field">
        <%= f.check_box :remember_me %> Recordarme
        </div>

      <%= render "users/shared/links" %>

  <% end %>


      </div>
      </div>
  </div>
</div>
</div>
<br>

<%end%>
<h3><%= link_to "Volver", :back , class: "btn btn-info", style: "color:white" %> </h3>





